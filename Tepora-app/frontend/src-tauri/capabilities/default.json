{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"log:default",
		"process:default",
		{
			"identifier": "shell:allow-spawn",
			"allow": [
				{
					"name": "binaries/tepora-backend",
					"sidecar": true,
					"args": true
				}
			]
		}
	]
}
